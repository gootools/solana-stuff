{
  "name": "@gootools/solana-stuff",
  "version": "0.0.6",
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/mjs/index.d.ts",
  "exports": {
    "import": "./dist/mjs/index.js",
    "require": "./dist/cjs/index.js"
  },
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.json && tsc -p tsconfig.cjs.json && ./bin/after-build.sh",
    "test": "jest",
    "release": "npm run build && release-it",
    "format": "prettier --write  '*.{json,js}' 'src/**/*.{js,ts}'",
    "lint": "eslint --max-warnings=0 src && prettier --check '*.{json,js}' 'src/**/*.{js,ts}' 'test/**/*.{js,ts}'"
  },
  "license": "MIT",
  "devDependencies": {
    "@solana/web3.js": "^1",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "bn.js": "^5",
    "esbuild": "^0.14.27",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-typescript": "^3.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.0",
    "release-it": "^14.12.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1",
    "bn.js": "^5"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gootools/solana-stuff.git"
  },
  "bugs": {
    "url": "https://github.com/gootools/solana-stuff/issues"
  },
  "homepage": "https://github.com/gootools/solana-stuff#readme",
  "eslintConfig": {
    "root": true,
    "extends": [
      "typescript",
      "prettier"
    ]
  }
}
